sudo docker create \
--name=plex \
--net=host \
--restart=always \
-e VERSION=latest \
-e PUID=1001 -e PGID=1001 \
-e TZ=America/Toronto \
-v /home/docker/plex/config:/config \
-v /home/docker/plex/tvshows:/tvshows \
-v /home/docker/plex/movies:/movies \
-v /home/docker/plex/transcode:/transcode \
linuxserver/plex