sudo docker create \
  --name=sabnzbd \
  -e PUID=1000 \
  -e PGID=1000 \
  -e TZ=America/Toronto \
  -p 8080:8080 \
  -p 9090:9090 \
  -v path to data:/config \
  -v path to downloads:/home/docker/downloads \
  -v path to incomplete downloads:/home/docker/incomplete-downloads `#optional` \
  --restart unless-stopped \
  linuxserver/sabnzbd:latest